    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "npm run copy-env && nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:mainnet": "npm run copy-mainnet && node dist/main",
        "start:testnet": "npm run copy-testnet && node dist/main",
        "start:devnet": "npm run copy-devnet && node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "copy-env": "cp .env.example .env",
        "copy-mainnet": "cp .env.mainnet .env",
        "copy-testnet": "cp .env.testnet .env",
        "copy-devnet": "cp .env.devnet .env",
        "test": "jest --verbose false --config ./test/jest-config.json --forceExit",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json"
    },
